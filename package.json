{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "VITE_EMUL=true vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "VITE_EMUL=true vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "emulators:start": "firebase emulators:start --only=firestore,auth --import=./firestore-emulators",
    "emulators:start:test": "firebase emulators:start --only=firestore,auth --import=./firestore-emulators-test",
    "emulators:export": "firebase emulators:export ./firestore-emulators",
    "emulators:export:test": "firebase emulators:export ./firestore-emulators-test",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "ts": "yarn tsc --incremental --noEmit --pretty",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "VITE_EMUL=true vite --open --mode ssr",
    "start:production": "vite --open --mode ssr",
    "qwik": "qwik",
    "test:component": "TZ=UTC yarn cypress open --component --browser electron",
    "test:e2e": "TZ=UTC yarn cypress run --e2e --browser electron",
    "test:all": "TZ=UTC yarn cypress run --e2e --browser chrome --config baseUrl=http://localhost:4173 && TZ=UTC yarn cypress run --component --browser chrome",
    "test:all:ci": "TZ=UTC yarn start-server-and-test 'yarn emulators:start:test' 127.0.0.1:4000 'yarn preview' http://localhost:4173 'yarn test:all'",
    "test": "yarn start-server-and-test emulators:start:test 127.0.0.1:4000 test:component"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.1.4",
    "@builder.io/qwik-city": "1.1.4",
    "@testing-library/cypress": "^9.0.0",
    "@types/eslint": "8.40.0",
    "@types/node": "^20.2.3",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "autoprefixer": "10.4.14",
    "cypress": "^13.6.2",
    "cypress-ct-qwik": "^0.3.0",
    "cypress-wait-until": "^2.0.1",
    "eslint": "8.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "1.1.4",
    "node-fetch": "3.3.1",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.4",
    "undici": "5.22.1",
    "vercel": "^29.4.0",
    "vite": "4.3.8",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@marplex/flarebase-auth": "^1.2.0",
    "@qwikest/icons": "^0.0.8",
    "@sagi.io/workers-jwt": "^0.0.23",
    "cookie": "^0.5.0",
    "daisyui": "^3.7.3",
    "dayjs": "^1.11.9",
    "firebase": "^10.3.1",
    "immer": "^10.0.2",
    "nanoid": "^4.0.2"
  }
}
