{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "emulators:start": "firebase emulators:start --only=firestore,auth --import=./firestore-emulators",
    "emulators:export": "firebase emulators:export ./firestore-emulators",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "ts": "yarn tsc --incremental --noEmit --pretty",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "DEV=true qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "start:production": "VITE_DISABLE_FIREBASE_EMULATORS=true vite --open --mode ssr",
    "test.e2e": "playwright test",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.0.0",
    "@builder.io/qwik-city": "1.0.0",
    "@playwright/test": "^1.32.3",
    "@testing-library/cypress": "^9.0.0",
    "@types/eslint": "8.37.0",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "autoprefixer": "10.4.14",
    "cypress": "^12.11.0",
    "cypress-ct-qwik": "^0.3.0",
    "eslint": "8.38.0",
    "eslint-plugin-qwik": "0.23.0",
    "firebase-admin": "^11.5.0",
    "node-fetch": "3.3.1",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "undici": "5.21.2",
    "vercel": "^29.1.0",
    "vite": "4.3.4",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@marplex/flarebase-auth": "^1.2.0",
    "@sagi.io/workers-jwt": "^0.0.23",
    "cookie": "^0.5.0",
    "daisyui": "^2.51.6",
    "dayjs": "^1.11.7",
    "firebase": "^9.21.0",
    "nanoid": "^4.0.2"
  }
}
